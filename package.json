{
  "name": "react-redux-starter-kit",
  "version": "0.8.0",
  "description": "Boilerplate to get you started with React, Redux, React-Router, and Koa",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack",
    "start": "npm run dev",
    "dev": "node --harmony bin/webpack-dev-server",
    "dev:debug": "npm run dev -- --debug",
    "dev:debugnw": "npm run dev -- --debug --nw",
    "test": "npm run test:unit",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "deploy": "npm run test:unit && npm run compile"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.7.4",
    "object-assign": "^4.0.0",
    "react": "^0.14.0-beta3",
    "react-dom": "^0.14.0-beta3",
    "react-redux": "^2.0.0",
    "react-router": "1.0.0-beta3",
    "redux": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.8.20",
    "chai": "^3.2.0",
    "css-loader": "^0.17.0",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.1",
    "extract-text-webpack-plugin": "^0.8.0",
    "html-webpack-plugin": "^1.6.1",
    "karma": "^0.13.8",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.6.0",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "phantomjs": "^1.9.17",
    "phantomjs-polyfill": "0.0.1",
    "react-hot-loader": "^1.2.8",
    "redux-devtools": "^2.0.0",
    "sass-loader": "^2.0.0",
    "style-loader": "^0.12.1",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^3.18.0"
  }
}
